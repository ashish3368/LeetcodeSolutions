#include<iostream>
using namespace std;
void reverseArray(int arr[], int size){
    int start = 0;
    int end = size-1;
    while(start<=end){
    swap(arr[start] , arr[end]);
    start++;
    end--;
}
}
void printarray(int arr[], int size){
    for(int i = 0;i<size;i++){
    cout<<arr[i]<<" ";
}
     cout<<endl;
}
int main(){
   int arr[5] = {1,2,3,46,5};
   reverseArray(arr,5);
   printarray(arr,5);
return 0;
}
